<?xml version="1.0"?>
<odoo>
		 
	<record id="seq_inter_transfer" model="ir.sequence">
		<field name="name">Inter Company Transfer</field>
		<field name="code">inter.transfer.company</field>
		<field name="prefix">INT/%(year)s/</field>
		<field name="padding">5</field>
		<field name="company_id" eval="False"/>
	</record>

	<record id="seq_return_inter_transfer" model="ir.sequence">
		<field name="name">Return Inter Company Transfer</field>
		<field name="code">return.inter.transfer.company</field>
		<field name="prefix">RINT/%(year)s/</field>
		<field name="padding">5</field>
		<field name="company_id" eval="False"/>
	</record>

</odoo>